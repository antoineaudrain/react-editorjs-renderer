{
  "name": "react-editorjs-renderer",
  "version": "1.0.1",
  "description": "React Editor.js Renderer, based on API and Open Source",
  "main": "dist/index.js",
  "module": "build/index.es.js",
  "author": "antoineaudrain",
  "license": "MIT",
  "repository": "antoineaudrain/react-editorjs-renderer",
  "bugs": {
    "url": "https://github.com/antoineaudrain/react-editorjs-renderer/issues"
  },
  "keywords": [
    "react"
  ],
  "scripts": {
    "build": "rimraf dist/* && npm run build:prod",
    "build:dev": "webpack --mode development --progress --display-error-details --display-entrypoints --watch",
    "build:prod": "webpack --mode production",
    "lint": "eslint src/",
    "lint:errors": "eslint src/ --quiet",
    "lint:fix": "eslint src/ --fix"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react-html-parser": "^2.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-react-jsx": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-class-display-name": "^2.1.0",
    "buffer": "^5.6.0",
    "eslint": "^7.11.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rimraf": "^3.0.2",
    "webpack": "^5.0.0-rc.6",
    "webpack-cli": "^4.1.0"
  },
  "files": [
    "build"
  ]
